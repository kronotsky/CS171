# convenient.
###############################################################################
CC = g++
FLAGS = -g
SOURCES = *.h *.cpp

EXENAME = my_prog

all: $(SOURCES)
	$(CC) $(FLAGS) -o $(EXENAME) $(SOURCES)

clean:
	rm -f *.o $(EXENAME)

.PHONY: all clean

